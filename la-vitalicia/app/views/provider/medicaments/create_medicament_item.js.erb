<%if @save%>
	$('.btn.btn-primary.create').button('reset');
	$('#medicaments-table').html("<%= escape_javascript(render 'provider/medicaments/medicaments_request') %>")
	$('#modal_medicament_item_request').modal('hide');
	$('#request-options').html("<%= escape_javascript(render 'provider/medicaments/options_request_active') %>")
<%else%>
	$('#alert-msg').html("<%= escape_javascript(render 'provider/medicaments/alert_error_medicament_item') %>")	
	$('.btn.btn-primary.create-item').button('reset');
<%end%>	